<?xml version="1.0" encoding="utf-8" ?>
<mct:Popup xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
           xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
           xmlns:mct="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
           x:Class="OnlineDanceStore.View.PopUpPage">
    <VerticalStackLayout >
        <Border BackgroundColor="#FBEAE6" Padding="4"  >
            <Image Source="{Binding ImagePath}"   Aspect="AspectFit"  HeightRequest="250" WidthRequest="250"/>
        </Border>

    </VerticalStackLayout>
    

</mct:Popup>